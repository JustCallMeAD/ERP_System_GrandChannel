
<div class="iframe-wo" style="margin:20px">
    <h1>Work Order</h1>
    <h2 class="h2-customercode"></h2>
    <button class="btn btn-primary btn-downloadwo" style="margin-top:20px">Download this wo</button>
    <div style="margin-top:20px;border:double;height:180px;padding:20px;padding-top:0px">
        <h3>Summary</h3>
        <div style="float:left;width:33%;border:dotted;border-color:red">
            <text>Place Time:</text><text class="text-place"> </text><br />
            <text>Ready Time:</text><text class="text-ready"> </text><br />
            <text>ETS:</text><text class="text-ets"> </text><br />
            <text>Pick Man:</text><text class="text-pm"> </text><br />
            <text>Instructor:</text><text class="text-instructor"> </text><br />
        </div>
        <div style="float:left;width:34%;border:dotted;border-color:darkseagreen">
            <text>Ship Order #:</text><text class="text-so"> </text><br />
            <text>Reference #:</text><text class="text-reference"> </text><br />
            <text>Destination:</text><text class="text-destination"> </text><br />
            <text>Carrier:</text><text class="text-carrier"> </text><br />
            <text>Lot:</text><text class="text-lot"> </text><br />
        </div>
        <div style="float:right;width:33%;border:dotted;border-color:blue">
            <text>Pickable Ctns:</text><text class="text-ctns"> </text><br />
            <text>Pickable Plts:</text><text class="text-plts"> </text><br />
            <text>Plts From Inventory:</text><text class="text-inventory"> </text><br />
            <text>New Plts:</text><text class="text-new"> </text><br />
            <text>Actual Outbound Plts:</text><text class="text-total"> </text><br />
        </div>
    </div>
    <div style="margin-top:20px;border:double;padding:20px;padding-top:0px">
        <h3>Picking List</h3>
        <table class="table table-condensed table-hover table-striped table-pickinglist">
            <thead>
                <tr>
                    <th>Order</th>
                    <th>Container</th>
                    <th>SKU</th>
                    <th>Pick Ctns</th>
                    <th>Pick Plts</th>
                    <th>Location</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
    <div style="margin-top:20px;border:double;padding:20px;padding-top:0px">
        <h3>Operation Instruction</h3>
        <button class="btn btn-primary btn-detail-create">Add new instruction</button>
        <button id="btn-detail-reset" class="btn btn-primary">Reset</button>
        <table class="table table-condensed table-hover table-striped table-wo">
            <thead>
                <tr>
                    <th style="width:50px">Id</th>
                    <th>Description</th>
                    <th>Comment</th>
                    <th>Result</th>
                    <th>Charging Status</th>
                    <th>Status</th>
                    <th>Operation</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
    <div style="text-align:center">
        <button class="btn btn-primary btn-push" style="margin-top:20px">Push</button>
    </div>
</div>

